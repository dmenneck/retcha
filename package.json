{
  "name": "retcha",
  "version": "0.0.0",
  "description": "A captcha component for React",
  "author": "Dirk Mennecke",
  "keywords": [],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/retcha.umd.js",
  "module": "./dist/retcha.es.js",
  "exports": {
    ".": {
      "import": "./dist/retcha.es.js",
      "require": "./dist/retcha.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.3",
    "@testing-library/react": "^12.1.3",
    "@vitejs/plugin-react": "^1.0.7",
    "c8": "^7.11.0",
    "eslint": "^8.9.0",
    "eslint-plugin-react": "^7.28.0",
    "jsdom": "^19.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^17.0.2",
    "vite": "^5.0.12",
    "vitest": "^0.3.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
